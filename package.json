{
  "name": "abogadosoft",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:api": "cd backend && bun run dev",
    "dev:full": "concurrently \"vite\" \"cd backend && bun run dev\"",
    "dev:electron": "concurrently \"vite\" \"cd backend && bun run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "vite build",
    "build:api": "cd backend && bun run build",
    "preview": "vite preview",
    "electron": "electron .",
    "setup": "npm install && cd backend && bun install && bunx prisma generate",
    "db:push": "cd backend && bunx prisma db push",
    "db:seed": "cd backend && bun run prisma:seed",
    "db:studio": "cd backend && bunx prisma studio"
  },
  "dependencies": {
    "@hocuspocus/provider": "^3.4.4",
    "@liveblocks/client": "^3.13.4",
    "@liveblocks/yjs": "^3.13.4",
    "@onlyoffice/document-editor-react": "^2.1.1",
    "@supabase/supabase-js": "^2.95.3",
    "@tiptap/extension-collaboration": "^3.19.0",
    "@tiptap/extension-collaboration-cursor": "^2.26.2",
    "@tiptap/extension-image": "^3.19.0",
    "@tiptap/extension-placeholder": "^3.19.0",
    "@tiptap/extension-table": "^3.19.0",
    "@tiptap/extension-table-cell": "^3.19.0",
    "@tiptap/extension-table-header": "^3.19.0",
    "@tiptap/extension-table-row": "^3.19.0",
    "@tiptap/extension-text-align": "^3.19.0",
    "@tiptap/extension-underline": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "docx-preview": "^0.3.7",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "socket.io-client": "^4.8.3",
    "superdoc": "^1.11.0",
    "y-prosemirror": "^1.3.7",
    "yjs": "^13.6.29"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.0",
    "electron": "^40.2.1",
    "supabase": "^2.76.3",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wait-on": "^8.0.0"
  }
}
